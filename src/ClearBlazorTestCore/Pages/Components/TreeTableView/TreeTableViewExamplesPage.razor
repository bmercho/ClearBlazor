@page "/TreeTableView"
@using ClearBlazorTest
<DocsExamplesPage DocsInfo=@(new NonVirtualizedTreeDocsInfo())>
    <CodeSnippet Title="Basic non virtualized tree (with 100 items)" 
                 Description="Note: The initial is delay is the test data being created"
                 Code=@(typeof(TreeTableViewExample).Name)
                 BackgroundColor="@Color.BackgroundGrey">
        <TreeTableViewExample />
    </CodeSnippet>
    <CodeSnippet Title="Virtualized tree (with 100000 items)" 
                 Description="Note: The initial is delay is the test data being created"
                 Code=@(typeof(TreeTableViewVirtualizeExample).Name)
                 BackgroundColor="@Color.BackgroundGrey">
        <TreeTableViewVirtualizeExample />
    </CodeSnippet>
</DocsExamplesPage>