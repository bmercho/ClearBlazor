@page "/NonVirtualizedList"
@using ClearBlazorTest
<DocsExamplesPage DocsInfo=@(new NonVirtualizedListDocsInfo())>
    <CodeSnippet Title="Basic non virtualized (with 100 items)" 
                 Code=@(typeof(NonVirtualizedExample).Name) BackgroundColor="@Color.BackgroundGrey">
        <NonVirtualizedExample />
    </CodeSnippet>
    <CodeSnippet Title="Non virtualized with data provider (with 100 items)" Code=@(typeof(NonVirtualizedDataProviderExample).Name) BackgroundColor="@Color.BackgroundGrey">
        <NonVirtualizedDataProviderExample />
    </CodeSnippet>
    <CodeSnippet Title="Non virtualized goto index" Code=@(typeof(NonVirtualizedGotoIndexExample).Name) BackgroundColor="@Color.BackgroundGrey">
        <NonVirtualizedGotoIndexExample />
    </CodeSnippet>
    <CodeSnippet Title="Non virtualized variable row size" Code=@(typeof(NonVirtualizedVariableRowSizeExample).Name) BackgroundColor="@Color.BackgroundGrey">
        <NonVirtualizedVariableRowSizeExample />
    </CodeSnippet>
</DocsExamplesPage>