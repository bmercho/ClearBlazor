@page "/NonVirtualizedList"
@using ClearBlazorTest
<DocsExamplesPage DocsInfo=@(new NonVirtualizedListDocsInfo())>
    <CodeSnippet Title="Basic non virtualized list (with 100 items)" 
                 Code=@(typeof(NonVirtualizedListExample).Name) BackgroundColor="@Color.BackgroundGrey">
        <NonVirtualizedListExample />
    </CodeSnippet>
    <CodeSnippet Title="Non virtualized list with data provider (with 100 items)" Code=@(typeof(NonVirtualizedListDataProviderExample).Name) BackgroundColor="@Color.BackgroundGrey">
        <NonVirtualizedListDataProviderExample />
    </CodeSnippet>
    <CodeSnippet Title="Non virtualized list with variable row size" Code=@(typeof(NonVirtualizedListVariableRowSizeExample).Name) BackgroundColor="@Color.BackgroundGrey">
        <NonVirtualizedListVariableRowSizeExample />
    </CodeSnippet>
</DocsExamplesPage>