@page "/CheckBox"
@using ClearBlazorTest
<DocsExamplesPage DocsInfo=@(new CheckBoxDocsInfo())>
        <TextBlock FontSize="50px" FontWeight=@((int)FontWeight.Normal) Margin="20">CheckBox</TextBlock>
        <StackPanel Padding="20" Spacing="30">
            <CodeSnippet Title="Basic" Code=@(typeof(CheckBoxSimpleExample).Name)>
                <WrapPanel Margin="10" ColumnSpacing="20">
                    <CheckBoxSimpleExample />
                </WrapPanel>
            </CodeSnippet>
            <CodeSnippet Title="Disabled" Code=@(typeof(CheckBoxDisabledExample).Name)>
                <WrapPanel Margin="10" ColumnSpacing="20">
                    <CheckBoxDisabledExample />
                </WrapPanel>
            </CodeSnippet>
            <CodeSnippet Title="ReadOnly" Code=@(typeof(CheckBoxReadonlyExample).Name)>
                <WrapPanel Margin="10" ColumnSpacing="20">
                    <CheckBoxReadonlyExample />
                </WrapPanel>
            </CodeSnippet>
            <CodeSnippet Title="Size" Code=@(typeof(CheckBoxSizeExample).Name)>
                <WrapPanel Margin="10" ColumnSpacing="20">
                    <CheckBoxSizeExample />
                </WrapPanel>
            </CodeSnippet>
            <CodeSnippet Title="Label" Code=@(typeof(CheckBoxLabelExample).Name)>
                <WrapPanel Margin="10" ColumnSpacing="20">
                    <CheckBoxLabelExample />
                </WrapPanel>
            </CodeSnippet>
            <CodeSnippet Title="Icons" Code=@(typeof(CheckBoxIconsExample).Name)>
                <WrapPanel Margin="10" ColumnSpacing="20">
                    <CheckBoxIconsExample />
                </WrapPanel>
            </CodeSnippet>
        </StackPanel>
</DocsExamplesPage>