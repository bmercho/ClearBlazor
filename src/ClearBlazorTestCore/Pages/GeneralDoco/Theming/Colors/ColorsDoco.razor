@page "/Colors"
@namespace ClearBlazorTest
@using System.IO;
@using System.Reflection;
@using ClearBlazor

<StackPanel Padding=20 Orientation=Orientation.Portrait>
    <MarkdownViewer MarkdownString=@markdownString />
</StackPanel>



@code {
        string markdownString = string.Empty;

    protected override void OnInitialized()
    {
        base.OnInitialized();

        markdownString = ResourceFile.ReadResourceFile("Colors.md");
    }
}